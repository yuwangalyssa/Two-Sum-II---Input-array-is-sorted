class Solution {
public:
    vector<int> twoSum(vector<int>& numbers, int target) {    
        vector<int>ans;
        map<int, int>myMap;       
        for (int i = 0; i < numbers.size(); i++){
            if(myMap.find(target - numbers[i] )!=myMap.end()){
                ans.push_back(myMap[target-numbers[i]]);
                ans.push_back(i+1);
            }         
            myMap[numbers[i]] = i + 1;
        }
        return ans;
    }
};
